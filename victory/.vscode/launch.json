  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev",
      "request": "launch",
      "type": "dart",
      "toolArgs": ["--dart-define", "ENV=DEV"]
    },
    {
      "name": "dev profile mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "toolArgs": ["--dart-define", "ENV=DEV"]
    },
    {
      "name": "dev release mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": ["--dart-define", "ENV=DEV"]
    },
    {
      "name": "uat",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": ["--dart-define", "ENV=UAT"]
    },
    {
      "name": "production",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": ["--dart-define", "ENV=PROD"]
    }
  ]
}
